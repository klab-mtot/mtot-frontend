<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.nothing.NothingFragment">

    <androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/field_next"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="25dp"
        android:layout_marginEnd="373dp"
        android:layout_marginStart="17dp"
        android:layout_marginBottom="678dp"
        >
        <ImageView
            android:id="@+id/button_next"
            android:src="@drawable/ic_notifications_black_24dp"
            android:layout_marginStart="0dp"
            android:layout_width="match_parent "
            android:layout_height="match_parent"

            />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <TextView
        android:id="@+id/extra_forCommit"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="25sp"
        android:text=""
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


        
    <TextView
        android:id="@+id/text_addGroup"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="25dp"
        android:layout_marginEnd="255dp"
        android:textAlignment="center"
        android:textSize="20sp"
        android:text="그룹 추가"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />



    <androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/field_writeName"
        android:layout_width="365dp"
        android:layout_height="75dp"
        android:layout_marginTop="50dp"
        android:layout_marginStart="0dp"
        android:layout_marginBottom="728dp"

        app:layout_constraintStart_toStartOf="@+id/text_addGroup"
        app:layout_constraintTop_toTopOf="@+id/text_addGroup"
        >
        <TextView
            android:id="@+id/text_addName"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="0dp"
            android:layout_marginStart="4dp"
            android:textSize="13sp"
            android:text="그룹 추가"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
        <EditText
            android:id="@+id/test_writeName"
            android:layout_width="370dp"
            android:layout_height="50dp"
            android:layout_marginTop="10dp"
            android:text="여정 이름"


            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            />
    </androidx.constraintlayout.widget.ConstraintLayout>


    <androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/field_writeMember"
        android:layout_width="365dp"
        android:layout_height="75dp"
        android:layout_marginTop="95dp"
        android:layout_marginStart="0dp"
        android:layout_marginBottom="728dp"

        app:layout_constraintStart_toStartOf="@+id/field_writeName"
        app:layout_constraintTop_toTopOf="@+id/field_writeName"
        >
        <TextView
            android:id="@+id/text_addMember"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="0dp"
            android:layout_marginStart="4dp"
            android:textSize="13sp"
            android:text="그룹 멤버"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
        <EditText
            android:id="@+id/text_writeMember"
            android:layout_width="370dp"
            android:layout_height="50dp"
            android:layout_marginTop="15dp"
            android:text="검색"


            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            />

        <androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
            android:id="@+id/field_search"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="0dp"
            android:layout_marginStart="340dp"
            android:layout_marginBottom="25dp"
            >
            <ImageView
                android:id="@+id/button_search"
                android:src="@drawable/ic_notifications_black_24dp"
                android:layout_marginStart="0dp"
                android:layout_width="match_parent"
                android:layout_height="match_parent"

                />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>


    <androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/field_add"
        android:layout_width="220dp"
        android:layout_height="35dp"
        android:layout_marginTop="280dp"
        android:layout_marginBottom="728dp"
        android:layout_marginStart="70dp"

        app:layout_constraintStart_toStartOf="@+id/field_writeName"
        app:layout_constraintTop_toTopOf="@+id/field_writeName"
        >

        <TextView
            android:id="@+id/text_add"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:textAlignment="center"
            android:textSize="15sp"
            android:text="추가"

            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>

